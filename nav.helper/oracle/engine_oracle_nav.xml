<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GSQLNavTree" id="ROOT">
  	<child>
		<object class="GSQLNavTree"  id="SESSION_PRIVILEGES">
			<property name="child_id_name">SESSION_PRIVILEGE</property>
			<property name="stock_name">GSQLE_ORACLE_STOCK_SESSION_PRIVILEGES</property>
			<property name="name" translatable="yes">"Session Privileges"</property>
			<query>
					select 
					privilege from session_privs where privilege like :name order by privilege
			</query>
			<query version="9">
					select version=9
					privilege from session_privs where privilege like :name order by privilege
			</query>
		</object>
	</child>
	<child>
		<object class="GSQLNavTree"  id="ENABLED_ROLES">
			<property name="child_id_name">ENABLED_ROLE</property>
			<property name="stock_name">GSQLE_ORACLE_STOCK_ENABLED_ROLES</property>
			<property name="name" translatable="yes">"Enabled Roles"</property>
			<query>
					select role from session_roles where role like :name order by role
			</query>
			<query version="9">
					select version=9, role from session_roles where role like :name order by role
			</query>
			<child>
				<object class="GSQLNavTree" id="GRANTED_ROLES">
					<property name="child_id_name">GRANTED_ROLE</property>
					<property name="stock_name">GSQLE_ORACLE_STOCK_GRANTED_ROLES</property>
					<property name="name" translatable="yes">"Granted Roles"</property>
					<query version="10">
						select version=10, role from session_roles where role like :name order by role
					</query>
				</object>
			</child>
			<child>
				<object class="GSQLNavTree" id="SYSTEM_PRIVELEGES">
					<property name="child_id_name">SYSTEM_PRIVELEGE</property>
					<property name="stock_name">GSQLE_ORACLE_STOCK_SYSTEM_PRIVELEGES</property>
					<property name="name" translatable="yes">"System Privileges"</property>
					<query>
						select SYSTEM PRIVILEGES role like :name order by role
					</query>
				</object>
			</child>
			<child>
				<object class="GSQLNavTree"  id="OBJECT_PRIVELEGES">
					<property name="child_id_name">OBJECT_PRIVELEGE</property>
					<property name="stock_name">GSQLE_ORACLE_STOCK_OBJECT_PRIVELEGES</property>
					<property name="name" translatable="yes">"Object Privileges"</property>
					<query>
						select OBJECT PRIVILEGES role like :name order by role
					</query>
				</object>
			</child>
	
		</object>
	</child>
	<child>
		<object class="GSQLNavTree" id="MY_SCHEMA">
			<property name="stock_name">GSQL_STOCK_MY_SCHEMA</property>
			<property name="name" translatable="yes">"My Schema"</property>
		
			<child>
					<object class="GSQLNavTree" id="TABLES">
						<property name="child_id_name">TABLE</property>
						<property name="stock_name">GSQL_STOCK_TABLES</property>
						<property name="name" translatable="yes">"Tables"</property>			
						<query>
							select TABLES from oracle
						</query>
						
					</object>
			</child>

		</object>
	</child>
	
  </object>
</interface>
