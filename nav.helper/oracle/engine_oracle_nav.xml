<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GSQLNavTree" id="ORACLE_ROOT">
  	<property name="id">ORACLE_ROOT</property>
  	
  	<child>
		<object class="GSQLNavTree"  id="ORACLE_SESSION_PRIVILEGES">
			<property name="id">ORACLE_SESSION_PRIVILEGES</property>
			<property name="child_id">ORACLE_SESSION_PRIVILEGE</property>
			<property name="stock_name">GSQLE_ORACLE_STOCK_SESSION_PRIVILEGES</property>
			<property name="name" translatable="yes">"Session Privileges"</property>
			<query>
					select 
					privilege from session_privs where privilege like :name order by privilege
			</query>
			<query version="9">
					select version=9
					privilege from session_privs where privilege like :name order by privilege
			</query>
		</object>
		
	</child>
	<child>
		<object class="GSQLNavTree"  id="ORACLE_ENABLED_ROLES">
			<property name="id">ORACLE_ENABLED_ROLES</property>
			<property name="child_id">ORACLE_ENABLED_ROLE</property>
			<property name="stock_name">GSQLE_ORACLE_STOCK_ENABLED_ROLES</property>
			<property name="name" translatable="yes">"Enabled Roles"</property>
			<query>
					select role from session_roles where role like :name order by role
			</query>
			<query version="9">
					select version=9, role from session_roles where role like :name order by role
			</query>
			<child>
				<object class="GSQLNavTree" id="ORACLE_GRANTED_ROLES">
					<property name="id">ORACLE_GRANTED_ROLES</property>
					<property name="child_id">ORACLE_GRANTED_ROLE</property>
					<property name="stock_name">GSQLE_ORACLE_STOCK_GRANTED_ROLES</property>
					<property name="name" translatable="yes">"Granted Roles"</property>
					<query version="10">
						select version=10, role from session_roles where role like :name order by role
					</query>
				</object>
			</child>
			<child>
				<object class="GSQLNavTree" id="ORACLE_SYSTEM_PRIVELEGES">
					<property name="id">ORACLE_SYSTEM_PRIVELEGES</property>
					<property name="child_id">ORACLE_SYSTEM_PRIVELEGE</property>
					<property name="stock_name">GSQLE_ORACLE_STOCK_SYSTEM_PRIVELEGES</property>
					<property name="name" translatable="yes">"System Privileges"</property>
					<query>
						select SYSTEM PRIVILEGES role like :name order by role
					</query>
				</object>
			</child>
			<child>
				<object class="GSQLNavTree"  id="ORACLE_OBJECT_PRIVELEGES">
					<property name="id">ORACLE_OBJECT_PRIVELEGES</property>
					<property name="child_id">ORACLE_OBJECT_PRIVELEGE</property>
					<property name="stock_name">GSQLE_ORACLE_STOCK_OBJECT_PRIVELEGES</property>
					<property name="name" translatable="yes">"Object Privileges"</property>
					<query>
						select OBJECT PRIVILEGES role like :name order by role
					</query>
				</object>
			</child>
	
		</object>
	</child>
	<child>
		<object class="GSQLNavTree" id="ORACLE_MY_SCHEMA">
			<property name="id">ORACLE_MY_SCHEMA</property>
			<property name="stock_name">GSQL_STOCK_MY_SCHEMA</property>
			<property name="name" translatable="yes">"My Schema"</property>
		
			<child>
					<object class="GSQLNavTree" id="ORACLE_TABLES">
						<property name="id">ORACLE_TABLES</property>
						<property name="child_id">ORACLE_TABLE</property>
						<property name="stock_name">GSQL_STOCK_TABLES</property>
						<property name="name" translatable="yes">"Tables"</property>			
						<query>
							select TABLES from oracle
						</query>
						
					</object>
			</child>

		</object>
	</child>
	
	<child>
		<object class="GSQLNavTree" id="ORACLE_MY_SCHEMA_V8">
			<property name="id">ORACLE_MY_SCHEMA_V8</property>
			<property name="stock_name">GSQL_STOCK_MY_SCHEMA</property>
			<property name="name" translatable="yes">"My Schema"</property>
			<property name="version">10</property>			

		</object>
	</child>
	
  </object>
</interface>
