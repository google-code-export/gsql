<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkTreeStore" id="nav_treestore">
    <columns>
      <!-- column-name GSQL_NAV_TREE_ID -->
      <column type="guint"/>
      <!-- column-name GSQL_NAV_TREE_OWNER -->
      <column type="gchar"/>
      <!-- column-name GSQL_NAV_TREE_STOCK_NAME -->
      <column type="gchar"/>
      <!-- column-name GSQL_NAV_TREE_NAME -->
      <column type="gchar"/>
      <!-- column-name GSQL_NAV_TREE_REALNAME -->
      <column type="gchar"/>
      <!-- column-name GSQL_NAV_TREE_INFO -->
      <column type="gchar"/>
      <!-- column-name GSQL_NAV_TREE_CHILD -->
      <column type="gpointer"/>
      <!-- column-name GSQL_NAV_TREE_DETAILS -->
      <column type="gpointer"/>
    </columns>
  </object>
  <object class="GtkListStore" id="nav_liststore">
    <columns>
      <!-- column-name GSQL_STOCK_IMAGE -->
      <column type="gchararray"/>
      <!-- column-name GSQL_STOCK_NAME -->
      <column type="gchararray"/>
      <!-- column-name GSQL_STOCK_FILE_NAME -->
      <column type="gchararray"/>
    </columns>
  </object>
